@media only screen and ( min-device-width: 412px ){
.dog_get {
    background: url(../img/dog_03.png) no-repeat 9VW 0VH;
    background-size: 14%;
    }
.dog{
    background: url(../img/dog_03.png) no-repeat 9VW 0VH;
    background-size: 14%;
    }
    .inviteFriends {
    position: relative;
    height: 55vh;
    background: url(../img/cashPack_03.png) no-repeat center;
    background-size: 83%;
    margin-top: 1vh;
}
 .inviteFriends ul {
    position: absolute;
    top: 21vh;
    left: 22vw;
    color: #e94935;
    line-height: 3vh;
    }  
.casher span {
    position: absolute;
    top: 4vh;
    left: 50%;
    transform: translateX(-50%);
    color: #ec4d35;
    font-size: 2.3rem;
    font-weight: bold;
}  
.inviteFriends .inviteYou{
	position: absolute;
    top: 45vh;
    left: 13vw   
} 
#layerBody {
	top:10%;
}
.goodOne {
    position: absolute;
    top: 23vh;
    left: 33vw;
    color: #fff;
} 
.goodThree {
    position: absolute;
    top: 48vh;
    left: 33vw;
    color: #fff;
} 
.activityTime {
    position: absolute;
    bottom: 6vh;
    width: 100%;
    text-align: center;
}
.layerClose {
    position: absolute;
    bottom: -6vh;
    left: 50%;
    transform: translateX(-50%);
}
}
