@media only screen and ( max-device-width: 320px ){
.dog_get {
    background: url(../img/dog_03.png) no-repeat 3VW 0VH;
    background-size: 14%;
    font-size: 1.3rem;
    }
.dog{
	background: url(../img/dog_03.png) no-repeat 3VW 0VH;
