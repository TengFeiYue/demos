*{margin:0;padding:0}
a{text-decoration: none;}
ul{list-style: none;}
.p-0{padding:0}
.m-0{margin:0}
.p-t-0{padding-top:0}
.p-b-0{padding-bottom:0}
.p-l-0{padding-left:0}
.p-r-0{padding-right:0}
.m-t-0{margin-top:0}
.m-b-0{margin-bottom:0}
.b-w{background:#fff;}
.c-w{color:#fff;}

.wraper{
	background:url(../img_mid/midAutumBg.png) no-repeat center;
	background-size:cover;
	padding:0;
}
header{
	background:url(../img_mid/mid_header_02.png) no-repeat center;
	background-size:contain;
	height:30vh;
	position:relative;
}
.dataPro{
	position: absolute;
    bottom: 5vh;
    left: 50%;
    transform: translateX(-50%);
    width: 100vw;
    height: 5vh;
    line-height: 4vh;
    color: #660da9;
    font-size: 1.2rem;
    background: url(../img_mid/middleAutum_09.gif) no-repeat center;
    background-size: contain;
    text-align: center;
}	
.dataPro .red{
	font-size:2rem;
	font-weight: bold;
	color:#ff0000;
}
.enrollCondition{
	background: #fff;
    border-radius: .6rem;
    box-shadow: 0px 0px 8px #8f2efd inset;
    width: 95vw;
    color: #8f2efd;
    margin: 0 auto;
    font-size: 1rem;
    padding-bottom: 1rem;
}
.enrollCondition h3{
	font-size: 1.6rem;
    text-align: center;
    padding-top: 1.6rem;
}
.enrollCondition ul{
	padding-left:1rem;
}
.goRegister{
	width: 22vw;
    margin: 0 auto;
    background: #8f2efd;
    color: #fff;
    line-height: 2.3rem;
    text-align: center;
    border-radius: 1.5rem;
    box-shadow: 0 0 5px #fff inset;
}
.activityRules{
	color: #c8b9de;
    width: 100%;
    padding: 1.5rem;
}
.applyReport{
	width: 100vw;
    height: 5vh;
    line-height: 4vh;
    color: #660da9;
    font-size: 1.2rem;
    background: url(../img_mid/middleAutum_09.gif) no-repeat center;
    background-size: contain;
    text-align: center;
    margin:0 auto;
}
.title{
	width: 30vw;
    margin: 0 auto;
    background: url(../img_mid/wordBg_03.png) no-repeat center;
    background-size: contain;
    height: 15vh;
    line-height: 16vh;
    color: #fff;
    text-align: center;
    font-size: 1.5rem;
}
.datas{
	width:100%;
	padding:0 .6rem;
}
.datas .col-xs-4{	
	padding:0 .6rem;
}
.dl{
	position:relative;
}
.oldPrice{
	color: #c0a5ce;
    text-align: center;
    text-decoration: line-through;
    font-size: 1rem;
    line-height: 2rem;
}
.proName{
	width: 100%;
    position: absolute;
    left: 0;
    bottom: 1.6rem;
    background: #6f0baf;
    opacity: .5;
    color: #fff;
    text-align: center;
    font-size: .6rem;
    line-height: 2rem;
}
.shareFriends{
	background: #6c0bae;
    padding: 2vh 0;   
}
.shareFriends span{
	display: inline-block;
    color: #fff;
    margin-bottom: 1vh;
    border-left: 4rem solid #b585d7;
    border-right: 4rem solid #b585d7;
    padding: 0 2vw;
    line-height: 1px;
}
.shareFriends .friends{
	display:block;
	color:#bfa5cd;
	width:20vw;
	padding-left:5vw;
	background:url(../img_mid/middleAutum_35.png) no-repeat left center;
	background-size:contain;
	margin:0 auto;
}









