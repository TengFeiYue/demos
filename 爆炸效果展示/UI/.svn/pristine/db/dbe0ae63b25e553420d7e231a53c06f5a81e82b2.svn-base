          height: 100%; }
      .layer .layer-con .laycon .laycon-name {
        font-size: 36rpx;
        color: #fff;
        text-align: center; }
      .layer .layer-con .laycon .laycon-code {
        height: 280rpx;
        width: 320rpx;
        background: #fff;
        box-shadow: 0 1px 2px 0 #CFCFCF;
        margin: 30rpx auto; }
        .layer .layer-con .laycon .laycon-code image {
          display: block;
          width: 280rpx;
          height: 280rpx;
          margin: 0 auto; }
      .layer .layer-con .laycon .laycon-op {
        width: 320rpx;
        height: 60rpx;
        line-height: 60rpx;
        text-align: center;
        border: 2rpx solid #ececec;
        font-size: 24rpx;
        color: #333;
        margin: 0 auto;
        border-radius: 6rpx; }
