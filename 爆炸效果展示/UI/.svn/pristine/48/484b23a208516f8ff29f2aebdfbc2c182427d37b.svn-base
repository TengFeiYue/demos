page,
.body {
  height: 100%;
  font-family: '\5FAE\8F6F\96C5\9ED1', arial;
  background-color: #ffffff;
  font-size: 14px;
  overflow: hidden;
  display: flex;
  flex-direction: column; }
  page .ulWraper,
  .body .ulWraper {
    padding: 30rpx;
    text-align: center; }
    page .ulWraper .modelItems,
    .body .ulWraper .modelItems {
      display: inline-block;
      width: 200rpx;
      line-height: 60rpx;
      border: none;
