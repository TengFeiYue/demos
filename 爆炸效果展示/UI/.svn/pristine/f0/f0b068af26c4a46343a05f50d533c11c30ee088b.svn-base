@import "./../../search.wxss";
.header {
  width: 100%;
  height: 380rpx;
  padding-bottom: 22rpx;
  display: flex;
  flex-direction: column;
}
.header .up {
  flex: 4;
  background: #0092FF;
}
.header .down {
  flex: 5;
  background: #ffffff;
}
.header .pannal {
  background: #FFFFFF;
  border: 1px solid #C2E6F7;
  box-shadow: 0 4rpx 8rpx 0 rgba(153, 153, 153, 0.5);
  border-radius: 16rpx 16rpx 0 0;
  width: 90%;
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -45%;
}
.banner .swiper-box {
  height: 200rpx;
}
.banner .swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: 5rpx;
}
.banner .swiper-box .wx-swiper-dot {
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  display: inline-flex;
  margin-left: 20rpx;
  justify-content: space-between;
}
.banner .swiper-box .wx-swiper-dot::before {
  content: '';
  flex-grow: 1;
  background: rgba(255, 255, 255, 0);
  border: 1px solid rgba(255, 255, 255, 0.8);
  border-radius: 50%;
}
.banner .swiper-box .wx-swiper-dot-active::before {
  background: rgba(255, 255, 255, 0.8);
}
.content .around {
  text-align: center;
}
.content .around .subTitle {
  padding: 20rpx 0;
}
.content .around .subTitle text {
  font-size: 32rpx;
  color: #333333;
  letter-spacing: -0.38px;
  display: inline-block;
  padding: 0 20rpx;
  border-right: 46rpx solid #0092FF;
  border-left: 46rpx solid #0092FF;
  height: 3rpx;
  line-height: 1rpx;
}
.content .around .subTitle .subTitleIcon {
  width: 48rpx;
  height: 48rpx;
  position: relative;
  right: -20rpx;
  top: 12rpx;
}
.content .around .aboutAround {
  padding: 2rpx 28rpx;
  border: 2rpx solid #ECECEC;
