.content .around .aboutAround .robot {
  height: 124rpx;
  width: 100%;
}
.content .around .aboutAround .robot .robot-bg {
  height: 124rpx;
  width: 100%;
  font-size: 24rpx;
  color: #4a4a4a;
  position: relative;
}
.content .around .aboutAround .robot .robot-bg .robot-bg {
  display: block;
  width: 100%;
  height: 80rpx;
}
.content .around .aboutAround .robot .robot-bg .robot-robot {
  display: block;
  width: 40%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  top: 0;
}
.content .around .aboutAround .robot .robot-bg .robot-back {
  display: block;
  height: 44rpx;
  width: 150rpx;
  position: absolute;
  bottom: 0;
}
.content .around .aboutAround .robot .robot-bg .robot-back.left {
  left: -28rpx;
}
.content .around .aboutAround .robot .robot-bg .robot-back.right {
  right: -28rpx;
  transform: rotateZ(180deg);
}
.content .around .aboutAround .robot .robot-bg text {
  line-height: 2;
  padding: 0 40rpx;
  border-right: 2rpx solid #ececed;
  border-left: 2rpx solid #ececed;
}
.content .around .aboutAround .aroundImg {
  width: 156rpx;
  height: 104rpx;
}
.content .around .aboutAround .aroundCompany {
  flex: 1;
  display: flex;
  flex-direction: row;
  justify-content: between;
  align-items: center;
  text-align: center;
  padding: 10rpx 20rpx 10rpx 0;
  border-right: 1px solid #ECECEC;
}
.content .around .aboutAround .aroundCompany .aroundData {
  position: relative;
  flex: 1;
  text-align: left;
}
.content .around .aboutAround .aroundCompany .aroundData .aroundText {
  padding-left: 15rpx;
  font-size: 28rpx;
  color: #4A4A4A;
  letter-spacing: -0.16px;
}
.content .around .aboutAround .aroundCompany .aroundData .aroundTxt {
  padding-left: 15rpx;
  font-size: 20rpx;
  color: #666666;
  letter-spacing: -0.13px;
}
.content .around .aboutAround .aroundCompany .aroundData:after {
  content: "";
  display: inline-block;
  width: 2rpx;
  height: 60rpx;
  background: #ECECEC;
  position: absolute;
  top: 5rpx;
  right: 28rpx;
}
.content .around .aboutAround .aroundCompany .aroundPic {
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.content .around .aboutAround .aroundCompany:nth-last-of-type(1) {
  border-right: none;
}
.content .recommand {
  text-align: center;
}
.content .recommand .subTitle {
  position: relative;
  padding: 20rpx 0;
  border-bottom: 2rpx solid #ECECEC;
}
.content .recommand .subTitle text {
  font-size: 32rpx;
  color: #333333;
  letter-spacing: -0.38px;
  display: inline-block;
  padding: 0 20rpx;
  border-right: 46rpx solid #0092FF;
  border-left: 46rpx solid #0092FF;
  height: 3rpx;
  line-height: 1rpx;
}
.content .recommand .subTitle .subTitleIcon {
  width: 48rpx;
  height: 48rpx;
  position: relative;
  right: -20rpx;
  top: 12rpx;
}
.content .recommand .subTitle .changeOthers {
  width: 120rpx;
  height: 52rpx;
  position: absolute;
  right: 40rpx;
  top: 30rpx;
}
.content .recommand .scrollBody .ul {
  padding: 14rpx 0rpx 110rpx 30rpx;
  box-sizing: border-box;
}
.content .recommand .scrollBody .ul .recommandList .companyName {
  text-align: left;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0.17px;
}
.content .recommand .scrollBody .ul .recommandList .companyName image {
  width: 25.2rpx;
  height: 26.8rpx;
  position: relative;
  right: -14rpx;
}
.content .recommand .scrollBody .ul .recommandList .applies {
  padding-top: 16rpx;
  font-size: 20rpx;
}
.content .recommand .scrollBody .ul .recommandList .applies .typesW {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  line-height: 36rpx;
  font-size: 20rpx;
  margin-right: 16rpx;
  text-align: center;
  border-radius: 50%;
  background: #9FDBF7;
  color: #ffffff;
  float: left;
}
.content .recommand .scrollBody .ul .recommandList .applies .types {
  display: inline-block;
  height: 36rpx;
  margin-right: 16rpx;
  text-align: center;
  border: 2rpx solid #C2E6F7;
  border-radius: 8rpx;
  width: 114rpx;
  line-height: 36rpx;
  float: left;
  background-color: #FFF;
  color: #333;
}
.content .recommand .scrollBody .ul .recommandList .otherDataService {
  font-size: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: between;
  align-items: center;
}
.content .recommand .scrollBody .ul .recommandList .otherDataService .otherserve {
  flex: 1.7;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.content .recommand .scrollBody .ul .recommandList .otherDataService .distance {
  flex: 0.3;
  text-align: right;
}
.content .recommand .scrollBody .ul .recommandList .companySite {
  font-size: 20rpx;
  text-align: left;
  padding: 14rpx 0;
}
.content .recommand .scrollBody .ul .tips {
  font-size: 28rpx;
  background: #ffffff;
  height: 60rpx;
  line-height: 60rpx;
  margin-bottom: 50rpx;
}
.content .recommand .scrollBody .ul .tips .loadShow {
  background: #ffffff;
  text-align: center;
  display: flex;
  flex-direction: row;
  align-items: center;
  align-self: center;
  justify-content: center;
}
.content .recommand .scrollBody .ul .tips .loadShow icon {
  position: relative;
  top: 17rpx;
  right: 5rpx;
}
.content .recommand .status {
  width: 100%;
  position: fixed;
  bottom: 9vh;
  left: 0;
  padding: 18rpx 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  align-items: center;
  justify-content: between;
}
.content .recommand .status .statusDes {
  padding-left: 30rpx;
  flex: 3;
  display: inline-block;
  font-size: 28rpx;
  color: #FFFFFF;
  letter-spacing: 0.2px;
  text-align: left;
}
.content .recommand .status .statusDes text {
  background: rgba(0, 0, 0, 0.2);
  color: rgba(255, 255, 255, 0.8);
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 10rpx;
}
.content .recommand .status .statusBtn {
  padding-right: 30rpx;
  flex: 1;
  display: inline-block;
  font-size: 28rpx;
  color: #FFFFFF;
  letter-spacing: 0.2px;
  text-align: right;
}
.content .recommand .status .statusBtn .btn {
  background: rgba(0, 0, 0, 0.2);
  color: rgba(255, 255, 255, 0.8);
  display: inline-block;
  height: 50rpx;
  line-height: 50rpx;
  padding: 0 10rpx;
  position: relative;
  right: 10rpx;
}
.content .recommand .status .statusBtn .shutDown {
  font-size: 40rpx;
}
