
page,
.body {
    height: 100%;
    font-family: '\5FAE\8F6F\96C5\9ED1', arial;
    background-color:#EEEEEE;
    font-size:14px;
    overflow: hidden;
    display: flex;
    flex-direction: column;
}
   .head{
      background: #F0EFF5;
      display:flex;
      flex-direction:row;
      justify-content:space-between;
      items-align:center;
      height:44rpx;
      line-height:44rpx;
      padding:22rpx 20rpx;
   }
   .cityPos{
      flex:2;
      font-size: 32rpx;
      color: #0386FF;
      position:relative;
      padding-left:40rpx;
   }
   .cityPos image{
      width:30rpx;
      height:36rpx;
      position:absolute;
      top:5rpx;
      left:0;
   }
   .cares{
      flex:3;
      text-align:right;
      font-size: 32rpx;
      color: #0386FF;
      display:flex;
   }
   .cares .eye{
       flex:2;
      display:inline-block;
      padding-left:40rpx;
      position:relative;

   }
    .collect{
       flex:1;
      display:inline-block;
      padding-left:40rpx;
      position:relative;
   }
   .eye image{
      width:40rpx;
      height:32rpx;
      position:absolute;
      right:70rpx;
      top:3rpx;

   }
   .collect image{
      width:33rpx;
      height:30rpx;
      position:absolute;
      left:35rpx;
      top:8rpx;
   }
   .materialInfos{
     background: #FFFFFF;
     padding:30rpx;
     display:flex;
     flex-direction:row;
     justify-content:space-between;
     items-align:center;
   }
   .materialPic{
     flex:1
   }
   .materialPic image{
     width:80rpx;
     height:80rpx;
   }
   .materialInfo{
     flex:5
   }
   .h3{
       font-size: 34rpx;
       color: #222222;
       line-height:48rpx;
   }
   .price_display{
       font-size: 28rpx;
       color: #FE9900;
       line-height:40rpx;
       padding:15rpx 0;
   }
   .tradeAdd{
      font-size: 26rpx;
      color: #333333;
      line-height:36rpx;
      display:flex;
      flex-direction:row;
      justify-content:space-between;
      items-align:center;
   }
   .address{
       flex:3;
   }
   .tradeAdd .price_num{
        flex:2;
        font-size:26rpx;
        color:#999999;
        text-align:right;
   }
   .ul{
      padding-top:30rpx;
   }
   .ul .scrollWraper{
        max-height:860rpx;
        padding-bottom:20rpx;
        overflow-y:auto;
        background:#ffffff;
   }
   .priceList{
      background: #FFFFFF;
      padding:30rpx;
      display:flex;
      flex-direction:row;
      justify-content:space-between;
      items-align:center;
      border-bottom:1px solid #eeeeee;
   }
   .priceItemPic{
      flex:1;
   }
   .priceItemPic image{
      width:92rpx;
      height:102rpx;
   }
   .priceItemInfo{
       flex:4;
       padding-left:20rpx;
   }
   .priceContact{
      flex:2;
      text-align:right;
   }
   .name{
        display:inline-block;
        font-size:34rpx;
        color:#222222;
        vertical-align:top;
        padding-right:20rpx;

   }
   .sellType{
       font-size: 26rpx;
       color: #333333;
   }
   .companyName{
       font-size: 26rpx;
       color: #333333;
   }
   .settled{
       width:28rpx;
       height:32rpx;
       position:relative;
       top:5rpx;
       left:10rpx;

   }
   .reportedPrice{
     font-size: 26rpx;
     color: #FF9A00;
     display:inline-block;
     vertical-align:middle;
   }
   .hastex{
        width:78rpx;
        height:37rpx;
        font-size:26rpx;
        color:#999999;
        line-height:37rpx;
        display:inline-block;
          padding-left:10rpx;
          padding-right:10rpx;
        vertical-align:middle;

   }
   .where{
      font-size: 26rpx;
      color: #151515;
      line-height: 37rpx;
   }
   .times{
       font-size: 26rpx;
       color: #999999;
   }
   .contactBtn{
       width:146rpx;
       height:58rpx;
       line-height:58rpx;
       text-align:center;
       font-size: 26rpx;
       color: #0688FF;
       border: 2rpx solid #0386FF;
       border-radius: 6rpx;
       float:right;
       margin-top:40rpx;
   }
   .company{
     padding:12rpx 0;
   }
