.uploadCard {
  border-bottom: 30rpx solid #f2f2f2;
}
.uploadCard .preTips {
  background: #F2F2F2;
  padding: 30rpx;
  line-height: 34rpx;
  font-size: 24rpx;
  color: #666666;
  letter-spacing: -0.14px;
}
.uploadCard .preTips image {
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}
.uploadCard .preTips text {
  vertical-align: middle;
  margin-left: 10rpx;
}
.uploadCard .uploadDl {
  padding: 24rpx 32rpx;
  font-size: 28rpx;
  color: #4A4A4A;
  letter-spacing: -0.14px;
}
.uploadCard .uploadDl .uploadDd {
  padding: 26rpx 0 70rpx;
  text-align: center;
}
.uploadCard .uploadDl .uploadDd image {
  width: 160rpx;
  height: 160rpx;
}
.editInfomation {
  padding-left: 32rpx;
}
.editInfomation .editDl {
  padding: 24rpx 30rpx 24rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: between;
  align-items: center;
  border-bottom: 1px solid #ECECEC;
}
.editInfomation .editDl .editDt {
  flex: 20;
  font-size: 28rpx;
  color: #4A4A4A;
  letter-spacing: 0;
  line-height: 40rpx;
}
.editInfomation .editDl .editDt image {
  width: 48rpx;
  height: 48rpx;
  vertical-align: middle;
}
.editInfomation .editDl .editDt text {
  vertical-align: middle;
  margin-left: 18rpx;
}
.editInfomation .editDl .editDd input {
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: right;
}
.editInfomation .editDl .editDd .iptColor {
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: right;
}
.editInfomation .editDl .editDd .confirmDd {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
}
.editInfomation .editDl .editDd .confirmDd input {
  flex: 1;
}
.editInfomation .editDl .editDD image {
  width: 17rpx;
  height: 28rpx;
}
.editInfomation .switchDl {
  padding: 24rpx 30rpx 30rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: between;
  align-items: center;
  border-bottom: 1px solid #ECECEC;
}
.editInfomation .switchDl .switchDt {
  flex: 2;
  text-align: left;
}
.editInfomation .switchDl .switchDt .whether {
  font-size: 32rpx;
  color: #4A4A4A;
  letter-spacing: 0;
}
.editInfomation .switchDl .switchDt .motta {
  font-size: 24rpx;
  color: #666666;
  letter-spacing: 0;
  padding-top: 10rpx;
}
.editInfomation .switchDl .switchDd {
  text-align: right;
}
.editInfomation .switchDl .switchDd .wx-switch-input {
  width: 102rpx;
  height: 62rpx;
}
.editInfomation .switchDl .switchDd .wx-switch-input::before {
  width: 102rpx;
  height: 60rpx;
}
.editInfomation .switchDl .switchDd .wx-switch-input::after {
  width: 60rpx;
  height: 55rpx;
}
.editInfomation .formUploadDl {
  padding: 24rpx 32rpx;
  font-size: 32rpx;
  color: #4A4A4A;
  letter-spacing: -0.14px;
}
.editInfomation .formUploadDl .formUploadDd {
  padding: 26rpx 0 70rpx;
  text-align: center;
}
.editInfomation .formUploadDl .formUploadDd image {
  width: 160rpx;
  height: 160rpx;
}
.confirmSubmit button {
  background: #0092FF;
  height: 96rpx;
  line-height: 96rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  letter-spacing: -0.19px;
  border-radius: 0;
}
.changeInfoLayer {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #000000;
  opacity: .6;
  z-index: 100;
}
.OperateLayerCon {
  position: fixed;
  width: 100%;
  top: 25%;
  z-index: 1000;
  text-align: center;
}
.OperateLayerCon .ConDetail {
  display: inline-block;
  width: 80%;
  background: #ffffff;
  border-radius: 8rpx;
  padding: 30rpx;
}
.OperateLayerCon .ConDetail .layerTitle {
  font-size: 36rpx;
  color: #333333;
  letter-spacing: -0.43px;
  padding-bottom: 20rpx;
}
.OperateLayerCon .ConDetail .layerDl {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: -0.38px;
  border-bottom: 1px solid #ECECEC;
  padding: 20rpx 0;
}
.OperateLayerCon .ConDetail .layerDl .layerDt {
  flex: 3;
  text-align: left;
}
.OperateLayerCon .ConDetail .layerDl picker {
  flex: 8;
}
.OperateLayerCon .ConDetail .layerDl .layerDd {
  flex: 6;
}
.OperateLayerCon .ConDetail .layerDl .layerDd .pClass {
  font-size: 32rpx;
  color: #999999;
  letter-spacing: -0.38px;
  text-align: left;
}
.OperateLayerCon .ConDetail .auditRes {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: -0.38px;
  border-bottom: 1px solid #ECECEC;
  padding: 20rpx 0;
}
.OperateLayerCon .ConDetail .auditRes .layerDt {
  flex: 3;
  text-align: left;
}
.OperateLayerCon .ConDetail .auditRes .layerDd {
  flex: 6;
  font-size: 32rpx;
  color: #999999;
  letter-spacing: -0.38px;
  text-align: left;
}
.OperateLayerCon .ConDetail .upload {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: -0.38px;
  padding: 30rpx 0;
}
.OperateLayerCon .ConDetail .upload .uploadDl {
  flex: 1;
  text-align: left;
}
.OperateLayerCon .ConDetail .upload .uploadImg {
  flex: 6;
}
.OperateLayerCon .ConDetail .upload .uploadImg image {
  width: 260rpx;
  height: 150rpx;
}
.OperateLayerCon .ConDetail .operateBtn {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding-top: 30rpx;
  font-size: 36rpx;
  color: #FFFFFF;
  letter-spacing: -0.19px;
}
.OperateLayerCon .ConDetail .operateBtn .opCancel {
  flex: 3;
  display: inline-block;
  height: 88rpx;
  background: #CCCCCC;
  border-radius: 8rpx;
  text-align: center;
  line-height: 88rpx;
}
.OperateLayerCon .ConDetail .operateBtn .gap {
  flex: 1;
}
.OperateLayerCon .ConDetail .operateBtn .opSubmit {
  flex: 3;
  display: inline-block;
  height: 88rpx;
  background: #0092FF;
  border-radius: 8rpx;
  text-align: center;
  line-height: 88rpx;
}
.OperateLayerCon .ConDetail .layercontent {
  font-size: 32rpx;
  color: #999999;
  letter-spacing: -0.38px;
  line-height: 48rpx;
  text-align: center;
  padding-bottom: 20rpx;
}
.OperateLayerCon .ConDetail .layerFooter {
  font-size: 36rpx;
  color: #0092FF;
  letter-spacing: -0.43px;
  border-top: 1px solid #ECECEC;
  padding-top: 30rpx;
}
.view {
  background: #333333;
  color: #ffffff;
  width: 270rpx;
  height: 270rpx;
  position: fixed;
  z-index: 1000000;
  left: 50%;
  top: 50%;
  border-radius: 10rpx;
  margin-top: -135rpx;
  margin-left: -135rpx;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}
.view .tipPic {
  flex: 1;
  padding-top: 36rpx;
}
.view .tipPic image {
  width: 100rpx;
  height: 100rpx;
}
.view text {
  flex: 1;
  padding: 0 15rpx;
}
