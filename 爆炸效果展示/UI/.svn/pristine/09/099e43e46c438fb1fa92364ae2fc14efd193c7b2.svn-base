    flex-direction: row;
    justify-content: space-between;
    align-items: center; }
    .confirmWraper .confirmDl .confirmDt {
      flex: 3;
      font-size: 32rpx;
      color: #000000;
      letter-spacing: 0;
      text-align: left; }
    .confirmWraper .confirmDl picker {
      flex: 8; }
    .confirmWraper .confirmDl .confirmDd {
      flex: 8; }
      .confirmWraper .confirmDl .confirmDd input {
        font-size: 30rpx;
        color: #888888;
        letter-spacing: 0;
        text-align: right;
        max-height: 50rpx; }
    .confirmWraper .confirmDl .confirmTo {
      flex: 1;
      text-align: center; }
      .confirmWraper .confirmDl .confirmTo image {
        width: 16rpx;
        height: 26rpx;
        position: relative;
        top: 5rpx; }
    .confirmWraper .confirmDl .pNameSelect {
