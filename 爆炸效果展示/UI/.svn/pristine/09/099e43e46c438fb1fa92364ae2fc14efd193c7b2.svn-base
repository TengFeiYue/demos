  .confirmWraper .btn-area {
    width: 100%;
    position: absolute;
    bottom: 0;
    left: 0;
    background: #eeeeee;
    padding: 50rpx 0; }
    .confirmWraper .btn-area .btnSub {
      width: 100%;
      height: 94rpx;
      background: #128EFF;
