page,
.body {
  height: 100%;
  font-family: '\5FAE\8F6F\96C5\9ED1', arial;
  background-color: #eeeeee;
  font-size: 14px;
  overflow: hidden;
  overflow-y: auto;
  display: flex;
  flex-direction: column; }

.hasConfirm {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  background: #FFFFFF;
  padding: 20rpx 30rpx;
  border-top: 1px solid #eeeeee; }
  .hasConfirm .antsooLogo {
    flex: 1;
    padding-right: 30rpx; }
    .hasConfirm .antsooLogo image {
      width: 92rpx;
      height: 102rpx; }
  .hasConfirm .txt {
    flex: 5;
    font-size: 28rpx;
    color: #000000; }
  .hasConfirm .toConfirm {
    display: inline-block;
    width: 128rpx;
    height: 48rpx;
    color: #0092FF;
    border: 1px solid #0092FF;
    border-radius: 8rpx;
    text-align: center;
    line-height: 48rpx;
    font-size: 24rpx; }

.upload {
  background: #FFFFFF;
  padding: 30rpx;
  margin: 30rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center; }
  .upload .uploadDl {
    flex: 3;
    font-size: 32rpx;
    color: #000000;
    letter-spacing: 0; }
  .upload .uploadImg {
    flex: 8;
    padding-left: 40rpx; }
    .upload .uploadImg image {
      width: 116rpx;
      height: 116rpx; }

.confirmWraper {
  position: relative;
  background: #ffffff;
  padding-left: 20rpx; }
  .confirmWraper .confirmDl {
    position: relative;
    padding: 15rpx 0;
