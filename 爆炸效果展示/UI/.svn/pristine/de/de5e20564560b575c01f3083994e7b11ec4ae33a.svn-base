.con {
  width: 100%; }
  .con .search-box {
    width: 100%;
    height: 88rpx;
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    background: rgba(0, 146, 255, 0.2);
    display: flex; }
    .con .search-box .search {
      width: 600rpx;
      height: 48rpx;
      background-color: #fff;
      border-radius: 16rpx;
      position: relative; }
      .con .search-box .search .icons {
        height: 48rpx;
        width: 48rpx;
        text-align: center;
        float: left; }
        .con .search-box .search .icons image {
          height: 50%;
          width: 50%; }
      .con .search-box .search input {
        display: block;
        height: 48rpx;
        min-height: 48rpx;
        width: 80%;
        font-size: 24rpx;
        color: #666;
        float: left; }
      .con .search-box .search .icons-del {
        height: 48rpx;
        width: 48rpx;
        float: right;
        text-align: center; }
        .con .search-box .search .icons-del image {
          height: 50%;
          width: 50%; }
    .con .search-box .cancel {
      height: 48rpx;
      width: 90rpx;
      line-height: 48rpx;
      text-align: center;
      font-size: 24rpx;
      color: #666; }
  .con .list-op {
    padding: 20rpx 30rpx;
    box-sizing: border-box;
    overflow: hidden; }
    .con .list-op .icons {
      width: 48rpx;
      height: 48rpx;
      float: left; }
      .con .list-op .icons image {
        width: 100%;
        height: 100%; }
    .con .list-op .text {
      width: 20%;
      height: 48rpx;
      line-height: 48rpx;
      float: left;
      font-size: 28rpx;
      color: #666;
      text-align: center; }
    .con .list-op .del {
      height: 48rpx;
      width: 48rpx;
      float: right; }
      .con .list-op .del image {
        width: 100%;
        height: 100%; }
  .con .list-detail {
    padding: 0rpx 30rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    color: #666; }
    .con .list-detail text {
      display: inline-block;
      height: 52rpx;
      line-height: 46rpx;
      padding: 0 20rpx;
      box-sizing: border-box;
      border: 2rpx solid #ececec;
      border-radius: 8rpx;
      box-sizing: border-box;
      margin-right: 20rpx;
      margin-bottom: 20rpx; }

